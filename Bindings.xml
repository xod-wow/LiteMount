<Bindings>

    -- Magic names causes it to click the LiteMount button with left mouse
    <Binding name="CLICK LM_B1:LeftButton" category="LiteMount">
    </Binding>

    <Binding name="CLICK LM_B2:LeftButton" category="LiteMount">
    </Binding>

    <Binding name="CLICK LM_B3:LeftButton" category="LiteMount">
    </Binding>

    <Binding name="CLICK LM_B4:LeftButton" category="LiteMount">
    </Binding>

    <Binding name="LM_FORCE_NEW_RANDOM" category="LiteMount">
      LiteMount:ForceNewRandomMount()
    </Binding>

</Bindings>
